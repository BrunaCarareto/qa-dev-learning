algoritmo "aula12 - soma procedimento"
// Data : 15/09/2020
// Seção de Declarações 
var
   x, y: inteiro
   
procedimento soma (a,b: inteiro)
inicio
      escreval ("Recebi o valor ", a)
      escreval ("Recebi o valor ", b)
      escreval ("A soma entre os dois é : ", a+b)
fimprocedimento
inicio
// Seção de Comandos 
   x <- 5
   y <- 3
   soma (x,y)
fimalgoritmo
