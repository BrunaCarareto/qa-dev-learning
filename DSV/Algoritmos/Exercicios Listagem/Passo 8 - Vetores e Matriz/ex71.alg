algoritmo "ex71"
// Data : 17/09/2020
// Seção de Declarações 
var
   c: inteiro
   v: vetor[0..7] de inteiro
inicio
// Seção de Comandos
   para c<-0 ate 7 faca
        v[c] <- 999
   fimpara

   escreva ("Vetor: ")
   para c<-0 ate 7 faca
        escreva ("[ ", v[c], " ]")
   fimpara
fimalgoritmo
