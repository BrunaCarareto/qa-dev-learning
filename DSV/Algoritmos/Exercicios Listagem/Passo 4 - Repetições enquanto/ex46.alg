algoritmo "ex46"
// Data : 07/09/2020
// Seção de Declarações 
var
   c, soma:inteiro
inicio
// Seção de Comandos
   c <- 4
   
   escreva ("Calculando:   ")
   enquanto (c < 100)faca
             c <- c + 2
             soma <- soma + c
             escreva (c, " +")

   fimenquanto
   escreval ("  ")
   escreval ("O resultado da soma é: ",soma)
fimalgoritmo
