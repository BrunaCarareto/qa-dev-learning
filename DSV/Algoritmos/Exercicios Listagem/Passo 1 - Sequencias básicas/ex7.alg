algoritmo "ex 7"
// Data : 24/08/2020
// Seção de Declarações 
var
   numero, dobro, parte: real
inicio
// Seção de Comandos
   escreva ("Digite um numero: ")
   leia (numero)
   
   dobro <- numero * 2
   parte <- numero /3
   
   escreval ("O dobro de ", numero, " é ", dobro)
   escreva ("A terça parte de ", numero, " é ", parte)
fimalgoritmo
