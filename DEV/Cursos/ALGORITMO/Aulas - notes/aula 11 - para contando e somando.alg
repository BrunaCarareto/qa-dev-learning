algoritmo "aula 11 - contando e somando "
// Data : 13/09/2020
// Seção de Declarações 
var
   n, c,s: inteiro
inicio
// Seção de Comandos 
   s <- 0
   para c <-1 ate 5 faca
        escreva ("Digite um valor: ")
        leia (n)
        s <- s + n
   fimpara
   escreval ("A soma dos valores é: ", s)
fimalgoritmo
