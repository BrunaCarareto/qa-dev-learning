algoritmo "ex42"
// Data : 07/09/2020
// Seção de Declarações
var
   c, n:inteiro
inicio
// Seção de Comandos

   escreva ("Escreva um numero inteiro positivo: ")
   leia (n)

   c <- 0
   enquanto (c < n) faca
            c <- c + 1
            escreva (c, "..")
   fimenquanto
   escreval ("  ")
   escreval ("Acabou!")
fimalgoritmo
