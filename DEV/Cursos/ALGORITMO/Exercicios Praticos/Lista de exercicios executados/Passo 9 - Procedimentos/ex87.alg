algoritmo "ex87"
// Data : 15/09/2020
// Seção de Declarações
var
   sms: caractere
procedimento gerador(var msg:caractere)
inicio
      escreval ("+---=====---+")
      escreval (msg)
      escreval ("+---=====---+")
fimprocedimento

inicio
// Seção de Comandos
   sms <- "Aprendendo Portugol"
   gerador(sms)
fimalgoritmo
