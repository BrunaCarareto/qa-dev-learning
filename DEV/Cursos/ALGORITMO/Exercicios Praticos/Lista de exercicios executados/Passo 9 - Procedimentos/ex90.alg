algoritmo "ex90"
// Data : 16/09/2020
// Seção de Declarações 
var
   n1, n2: inteiro
   
procedimento somador(x,y: inteiro)
inicio
      escreval ("A soma dos valores é: ", x+y)
fimprocedimento

inicio
// Seção de Comandos 
   escreva ("Digite o primeiro numero: ")
   leia (n1)
   escreva ("Digite o segundo numero: ")
   leia (n2)
   
   somador(n1,n2)
fimalgoritmo
