algoritmo "ex56"
// Data : 15/09/2020
// Seção de Declarações 
var
   num, soma: inteiro
inicio
// Seção de Comandos 
   num <-0
   soma <-0

   repita
         escreva ("Digite um numero: ")
         leia (num)
         soma <- soma + num
   ate (num=1111)
   escreval ("O resultado da SOMA dos numeros digitados é: ", soma)
fimalgoritmo
